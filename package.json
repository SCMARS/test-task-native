    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-universe": "^12.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.1.3"
